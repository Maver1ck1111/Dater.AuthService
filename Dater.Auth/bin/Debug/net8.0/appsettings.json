{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq"],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args" : {
          "serverUrl": "http://localhost:5341"
        }
      } 
    ],

    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },

    "Enrich": ["FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "AllowedHosts": "*"
}
