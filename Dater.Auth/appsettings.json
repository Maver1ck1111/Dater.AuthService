{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],

    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },

    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },

  "JWT": {
    "Audience": "https://localhost:5001",
    "Issuer": "Dater.AuthService",
    "Key": "This is a very long key that should be kept secret and not hardcoded in production code.",
    "ExpireMinutes": 10
  },
  "AllowedHosts": "*"
}
